#include<iostream>
#include<string>
#include<fstream>
struct scobca
{
	int lapki, dushki, kvadrat;
	string g;
	scobca(void)
	{
		lapki = 0; 
		dushki = 0;
		kvadrat=0;
	};
};
using namespace std;
int main()
{
	int k=1;
	string p;
	ifstream input("scobochki.txt");
	scobca *fil = new scobca[];
	for (int i = 0; i < k; i++)
	{
		int l = 0, d = 0, c = 0;
		getline(input, p, '\n');
		k++;
		for (int j = 0; j < p.size; j++)
		{
			// ((dvg)[dss"s"])[(])
			//(fy[nnb"lj]jh"agds]sh)
			if (p[j] == '(')
			{
				d++;
			}
			if (p[j] == '"')
			{
				l++;
				if (l == 2)
				{
					fil[i].lapki++;
					l = 0;
				}
			}
			if (p[j] == '[')
			{
				c++;
			}
			if (p[j] == ']')
			{
				if(!l==0)
				c--;
				fil[i].kvadrat++;
			}
			if (p[j] == ')')
			{
				d--;
				fil[i].dushki++;
			}
		}
		if (!l == 0 && !d == 0 && !c == 0)
			fil[i].g = "NO; ";
		else
			fil[i].g = "YES; ";
	}
	input.close();
	ofstream output("rez.txt");
	for (int i = 0; i < k - 1; i++)
	{
		if (fil[i].g == "NO; ")
			output << "NO\n";
		else
			output << "():" << fil[i].dushki << " []:" << fil[i].kvadrat << " "":" << fil[i].lapki << "\n";
	}
	output << "():" << fil[k-1].dushki << " []:" << fil[k - 1].kvadrat << " "":" << fil[k - 1].lapki;
	output.close();
}
